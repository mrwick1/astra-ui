{
  "name": "@astra-ui/astra-ui",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --continue",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && turbo run lint && turbo run test && changeset publish",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "check": "manypkg check"
  },
  "dependencies": {
    "@changesets/cli": "^2.29.8",
    "@manypkg/cli": "latest",
    "eslint": "^8.57.0",
    "prettier": "^3.8.1",
    "turbo": "^2.8.3"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "packageManager": "pnpm@9.15.4"
}
